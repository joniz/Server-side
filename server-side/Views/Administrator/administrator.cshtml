@model List<service.Models.Account>
@{
    ViewBag.Title = "administrator";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="panel panel-default">
    <!-- Default panel contents -->
    <div class="panel-heading" id="panelHeading">@using (Html.BeginForm("administrator", "Administrator", FormMethod.Get))
            {
        <div class="row">


            <form class="navbar-nav" role="search">

                <div class="col-lg-12 input-group add-on">

                    @Html.TextBox("Search", null, new { @class = "form-control input-lg", @placeholder = "Search", @type = "text" })
                    <div class="input-group-btn">
                        <button id="searchButton" class="btn btn-lg" type="submit"><i class="glyphicon glyphicon-search"></i></button>
                    </div>
                </div>

            </form>

        </div>
    }</div>
   
    <!-- Table -->
    <table class="table table-hover">


        <thead>
            <tr>
               
                <th>Admin-namn</th>
                <th>Admin-ID</th>
                
            </tr>
        </thead>



        <tbody>
            @foreach (var admins in Model)
            {


                <tr  onclick="document.location = '@Url.Action("administratorDetails", "Administrator", new {userName = @admins.Username})'">
                    <td>@admins.Username</td>
                    <td>@admins.Rank</td>
                    

                </tr>
            }
        </tbody>

    </table>
</div>