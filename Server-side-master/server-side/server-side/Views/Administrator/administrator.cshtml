
@{
    ViewBag.Title = "administrator";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="panel panel-default">
    <!-- Default panel contents -->
    <div class="panel-heading" id="panelHeading">Administrators</div>
   
    <!-- Table -->
    <table class="table table-hover">


        <thead>
            <tr>
               
                <th>Admin-namn</th>
                <th>Admin-ID</th>
                
            </tr>
        </thead>



        <tbody>
            @foreach (service.Models.Administrator admin in service.Models.Administrator.getAdministratorList())
            {


                <tr  onclick="document.location = '@Url.Action("administratorDetails", "Administrator", new {admId = @admin._admId})'">
                    <td>@admin._admUserName</td>
                    <td>@admin._admId</td>
                    

                </tr>
            }
        </tbody>

    </table>
</div>